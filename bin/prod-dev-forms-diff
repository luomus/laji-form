#!/bin/bash
form=$1
[[ -z $form ]] && echo "No form id given" && exit
echo -n 'form.laji.fi user: '
read user
echo -n 'form.laji.fi pass: '
read -s pass
echo "\n"
echo -n 'formtest.laji.fi user: '
read test_user
echo -n 'formtest.laji.fi pass: '
read -s test_pass
diff <(curl -s --user $user:$pass https://form.laji.fi/lajiform/"$form" | python -m json.tool) <(curl -s --user $test_user:$test_pass http://formtest.laji.fi/lajiform/"$form" | python -m json.tool)
