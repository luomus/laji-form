{
	"presets": ["react", "es2015", "stage-1"],
	"plugins": [
		"transform-object-rest-spread",
		"transform-class-properties",
		"transform-decorators-legacy"
	],
	"env": {
		"production": {
			"plugins": [
				["transform-runtime", {"polyfill": true, "regenerator": false}],
				"array-includes"
			]
		}
	}
}
