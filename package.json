{
  "name": "laji-form",
  "version": "0.0.605",
  "description": "React module capable of building dynamic forms from Laji form json schemas",
  "main": "lib/app.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline --host 0.0.0.0 --port 8083",
    "staging": "webpack-dev-server --content-base playground playground/app.js --host 0.0.0.0 --port 4010",
    "build:lib": "rimraf lib && NODE_ENV=production webpack --config webpack.config.lib.js && rimraf lib/styles.js",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "build": "npm run build:lib && npm run build:dist",
    "publish-to-npm": "npm version patch && git push && npm run build && npm publish",
    "push-to-git": "npm run lint && git push",
    "lint": "eslint -c .eslintrc.prod.json src playground",
    "test": "HOST=127.0.0.1 PORT=8083 TEST_BROWSER=chrome protractor protractor.conf.js",
    "test:debug": "HOST=127.0.0.1 PORT=8083 TEST_BROWSER=chrome HEADLESS=false node --inspect-brk node_modules/protractor/bin/protractor protractor.conf.js"
  },
  "keywords": [
    "react-jsonschema-form",
    "laji-form",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "repository": "https://bitbucket.org/luomus/laji-form.js.git",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "deep-equal": "^1.0.1",
    "deepmerge": "^1.1.1",
    "exif-js": "^2.3.0",
    "immutability-helper": "^2.1.2",
    "laji-map": "^1.0.494",
    "laji-validate": "0.0.73",
    "moment": "*",
    "prop-types": "^15.5.8",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "*",
    "react-addons-shallow-compare": "*",
    "react-autosuggest": "^9.3.2",
    "react-bootstrap": "v1.0.0-beta.8",
    "react-bootstrap-switch": "^15.0.4",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.4",
    "react-inlinesvg": "^0.6.0",
    "react-jsonschema-form": "git+https://git@github.com/olzraiti/react-jsonschema-form.git#d7e3482f2ad275a674ecb000793c579b12570235",
    "react-sortable-hoc": "^0.6.3",
    "react-spinner": "^0.2.7",
    "react-transition-group": "^1.1.1",
    "react-widgets": "^3.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.19.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-protractor": "^1.40.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "notus": "^0.3.2",
    "protractor": "^5.3.2",
    "protractor-console-plugin": "^0.1.1",
    "querystring": "^0.2.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "util": "^0.10.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  },
  "resolutions": {
    "laji-map": "1.0.494",
    "webdriver-manager": "12.1.4"
  }
}
