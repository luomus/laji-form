{
  "name": "laji-form",
  "version": "0.0.45",
  "description": "React module capable of building dynamic forms from Laji form json schemas",
  "main": "dist/laji-form.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline --port 8083",
    "staging": "webpack-dev-server --content-base playground playground/app.js --host 0.0.0.0 --port 4010",
    "build:lib": "rimraf lib && NODE_ENV=production babel -d lib/ src/",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "publish-to-npm": "npm version patch && git push && npm run build:dist && npm run build:lib && npm publish"
  },
  "keywords": [
    "react-jsonschema-form",
    "laji-form",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "repository": "https://bitbucket.org/luomus/laji-form.js.git",
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "deepmerge": "^1.1.1",
    "laji-map": "*",
    "moment": "^2.14.1",
    "rc-switch": "^1.4.2",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-update": "^15.3.2",
    "react-autosuggest": "^6.1.0",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.6.0",
    "react-inlinesvg": "^0.5.4",
    "react-jsonschema-form": "0.40",
    "react-masonry-component": "^4.2.1",
    "react-spinner": "^0.2.6",
    "react-widgets": "^3.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "querystring": "^0.2.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "util": "^0.10.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
