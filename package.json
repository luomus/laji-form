{
  "name": "laji-form",
  "version": "0.0.0",
  "description": "React module capable of building dynamic forms from Laji form json schemas",
  "main": "lib/app.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline",
    "staging": "webpack-dev-server --content-base playground playground/app.js --host 0.0.0.0 --port 4003",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js",
    "build:lib": "rimraf lib && NODE_ENV=production babel -d lib/ src/ --copy-files",
    "dist": "npm run build:dist && npm run build:lib",
    "install": "npm run build:lib"
  },
  "keywords": [
    "react-jsonschema-form",
    "laji-form",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "repository": "https://bitbucket.org/luomus/laji-form.js.git",
  "license": "MIT",
  "engines": {
    "node": "4.2.2"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "deepmerge": "~0.2.10",
    "moment": "^2.13.0",
    "react": "~15.0.1",
    "react-addons-update": "~15.0.1",
    "react-autosuggest": "^3.7.3",
    "react-dom": "~15.0.1",
    "react-jsonschema-form": "*",
    "react-widgets": "^3.3.1",
    "webpack": "^1.12.15"
  },
  "devDependencies": {
    "react-transform-hmr": "^1.0.4",
    "webpack-dev-server": "^1.14.1",
    "json-loader": "~0.5.4",
    "querystring": "^0.2.0",
    "util": "^0.10.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "file-loader": "^0.8.5",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "css-loader": "^0.23.1",
	  "es6-promise": "^3.2.1",
	  "isomorphic-fetch": "^2.2.1",
    "rimraf": "^2.4.4"
  }
}
