{
  "name": "laji-form",
  "version": "0.0.43",
  "description": "React module capable of building dynamic forms from Laji form json schemas",
  "main": "dist/laji-form.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline --port 8083",
    "staging": "webpack-dev-server --content-base playground playground/app.js --host 0.0.0.0 --port 4010",
    "build:lib": "rimraf lib && NODE_ENV=production babel -d lib/ src/ --copy-files",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "publish-to-npm": "npm version patch && git push && npm run build:dist && npm run build:lib && npm publish"
  },
  "keywords": [
    "react-jsonschema-form",
    "laji-form",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "repository": "https://bitbucket.org/luomus/laji-form.js.git",
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "deepmerge": "*",
    "laji-map": "*",
    "moment": "2.14.1",
    "rc-switch": "*",
    "react": "*",
    "react-addons-css-transition-group": "*",
    "react-addons-update": "*",
    "react-autosuggest": "*",
    "react-bootstrap": "*",
    "react-dom": "*",
    "react-dropzone": "^3.6.0",
    "react-inlinesvg": "^0.5.4",
    "react-jsonschema-form": "0.40",
    "react-masonry-component": "*",
    "react-spinner": "*",
    "react-widgets": "*"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-react-transform": "*",
    "babel-plugin-transform-class-properties": "*",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "extract-text-webpack-plugin": "^1.0.1",
    "es6-promise": "*",
    "isomorphic-fetch": "*",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.15.1",
    "style-loader": "*",
    "url-loader": "*",
    "file-loader": "0.9.0",
    "less-loader": "*",
    "less": "*",
    "css-loader": "*",
    "json-loader": "*",
    "querystring": "*",
    "react-transform-hmr": "*",
    "rimraf": "*",
    "util": "*"
  }
}
