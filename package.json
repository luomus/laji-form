{
  "name": "laji-form",
  "version": "0.0.11",
  "description": "React module capable of building dynamic forms from Laji form json schemas",
  "main": "dist/laji-form.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline",
    "staging": "webpack-dev-server --content-base playground playground/app.js --host 0.0.0.0 --port 4010",
    "build:lib": "rimraf lib && NODE_ENV=production babel -d lib/ src/ --copy-files",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "publish": "npm run build:dist && npm run build:lib"
  },
  "keywords": [
    "react-jsonschema-form",
    "laji-form",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "repository": "https://bitbucket.org/luomus/laji-form.js.git",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "*",
    "laji-map": "*",
    "moment": "*",
    "rc-switch": "*",
    "react": "*",
    "react-addons-update": "*",
    "react-addons-css-transition-group": "*",
    "react-autosuggest": "5.1.1",
    "react-bootstrap": "*",
	  "react-dom": "*",
    "react-jsonschema-form": "0.38",
    "react-masonry-component": "*",
    "react-spinner": "*",
    "react-widgets": "*"
  },
  "devDependencies": {
	  "babel-cli": "*",
	  "babel-core": "*",
	  "babel-loader": "*",
	  "babel-plugin-react-transform": "*",
	  "babel-plugin-transform-class-properties": "*",
	  "babel-plugin-transform-object-rest-spread": "*",
	  "babel-polyfill": "*",
	  "babel-preset-es2015": "*",
	  "babel-preset-react": "*",
	  "extract-text-webpack-plugin": "^1.0.1",
	  "es6-promise": "*",
	  "isomorphic-fetch": "*",
	  "webpack": "*",
	  "webpack-dev-server": "*",
	  "style-loader": "*",
	  "url-loader": "*",
	  "file-loader": "*",
	  "less-loader": "*",
	  "less": "*",
	  "css-loader": "*",
	  "json-loader": "*",
	  "querystring": "*",
	  "react-transform-hmr": "*",
	  "rimraf": "*",
	  "util": "*"
  }
}
